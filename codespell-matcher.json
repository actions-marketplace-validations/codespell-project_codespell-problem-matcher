{
	"problemMatcher": [
		{
			"owner": "codespell-matcher-error",
			"pattern": [
				{
					"regexp": "^(.+):(\\d+):\\s+(.+)$",
					"file": 1,
					"line": 2,
					"message": 3
				}
			]
		},
		{
			"owner": "codespell-matcher-warning",
			"pattern": [
				{
					"regexp": "^(WARNING):\\s+(.+?):\\s+(.+)$",
					"file": 3,
					"severity": 1,
					"message": 2
				}
			]
		}
	]
}
