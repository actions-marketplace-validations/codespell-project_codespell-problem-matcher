{
	"problemMatcher": [
		{
			"owner": "codespell-matcher-error",
			"pattern": [
				{
					"regexp": "^(.+):\\d+:\\s+(.+)$",
					"file": 1,
					"message": 2
				}
			]
		},
		{
			"owner": "codespell-matcher-warning",
			"pattern": [
				{
					"regexp": "^(WARNING):\\s+(.+?):\\s+(.+)$",
					"file": 3,
					"severity": 1,
					"message": 2
				}
			]
		}
	]
}
